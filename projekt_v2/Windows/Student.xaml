<Window x:Class="projekt_v2.Windows.Student"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projekt_v2.Windows"
        mc:Ignorable="d"
        Title="Student" Height="300" Width="300">
    <Grid Background="#FF2D306B">
        <ComboBox x:Name="cmbSelectQuiz" HorizontalAlignment="Left" Margin="47,127,0,0" VerticalAlignment="Top" Width="205" Loaded="cmbSelectQuiz_Loaded"/>
        <Label x:Name="label" Content="Select quiz: " HorizontalAlignment="Left" Margin="94,70,0,0" VerticalAlignment="Top" Foreground="White" FontSize="16" FontWeight="Bold"/>
        <Button x:Name="button" Content="Start" HorizontalAlignment="Left" Margin="100,204,0,0" VerticalAlignment="Top" Width="93" Click="button_Click" FontWeight="Bold" Height="33" Foreground="#FF040F48" Background="White" BorderBrush="White"/>
        <Label x:Name="label_Copy" Content="Welcome Student !" HorizontalAlignment="Left" Margin="48,27,0,0" VerticalAlignment="Top" Foreground="White" FontSize="20" FontWeight="Bold" Height="38" Width="204"/>
    </Grid>
</Window>
